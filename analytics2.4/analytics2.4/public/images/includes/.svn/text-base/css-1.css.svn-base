div#tipDiv {
  position:absolute; 
  visibility:hidden; 
  left:0; 
  top:0; 
  z-index:10000;
  background-color:#E8FCFF;
  border:1px solid #336; 
  width:250px;
  padding:4px;
  font-family:verdana;
  color:#000; font-size:11px;
  line-height:18px;
}
