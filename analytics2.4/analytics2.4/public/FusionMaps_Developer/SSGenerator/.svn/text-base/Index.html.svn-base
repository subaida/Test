<script language="javascript" type="text/javascript">
var isAuto=false;
var index1;
var maps;
maps=new FusionMapsSS();
function FC_Rendered(domId){
	
	maps.getReferenceToMap();
	maps.showSSFull('ssDiv');
}

function changeMap(index){
	//This method is called when the user changes the map in drop down.
	//Check if this index actually belongs to a map
	if (maps.mapList[index].isMap==true){
		maps.loadMap(index);
	}
}	
</script>
