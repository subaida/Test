div.sdmenu {
	width: 200px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	padding-bottom: 10px;
	background: url(../images/help_nav/bottom.gif) no-repeat  right bottom;
	color: #000000;
}
div.sdmenu div {
	background: url(../images/help_nav/title.gif) repeat-x;
	overflow: hidden;
}
div.sdmenu div:first-child {
	background: url(../images/help_nav/toptitle.gif) no-repeat;
}
div.sdmenu div.collapsed {
	height: 25px;
}
div.sdmenu div span {
	display: block;
	padding: 1px 10px;
	font-weight: bold;
	color: #000000;
	/*background: url(../images/help_nav/expanded.gif) no-repeat 10px center;*/
	cursor: default;
	border-bottom: 1px solid #D5E6EF;
}
div.sdmenu div.collapsed span {
/*	background-image:url(../images/help_nav/collapsed.gif);*/
}
div.sdmenu div a {
	padding: 5px 10px;
	background: #F0FAFF;
	display: block;
	border-bottom: 1px solid #D5E6EF;
	color: #000000;
	text-decoration:underline;
}
div.sdmenu div a.current {
	background : #F0FAFF;
}
div.sdmenu div a:hover {
	background : #F0FAFF;
	color: #000000;
	text-decoration: none;
	text-decoration:none;
}
div.sdmenu div strong{
	/*padding: 5px 10px;
	background: #F0FAFF;
