          </tr>

          <tr>
            <td height="5"></td>
            <td height="5"></td>
            <td height="5"></td>
          </tr>
          <tr>
            <td height="15"></td>
            <td height="15">
			<table width="100%" border="0" cellspacing="0" cellpadding="0">
			  <tr>
				<td align="LEFT"><a href="/admin_analytic/admin_home">Admin Home</a> &gt; <span class="nav_link">Unfilled IPs </span></td>
				<td align="RIGHT"><strong><a href="#" onclick="printWindow()"><img src="/images/analytic/print.png" border="0" /> </a></strong></td>
			  </tr>
			</table>
			 </td>
            <td height="15"></td>
          </tr>
          <tr>
            <td height="15"></td>
            <td height="15"></td>
            <td height="15"></td>
          </tr>
          <tr>
            <td height="15"></td>
            <td height="15">
			<table width="100%" border="0" cellspacing="0" cellpadding="0" >
			  <tr>
				<td align="CENTER">
					<table width="100%" border="0" cellspacing="5" cellpadding="5" class="subpagetableborder" >
              <tr>
                <td width="73%">
				<form  action="/admin_analytic/unfilled_ips_search" method="post" name="owner_report">
				<table width="100%" border="0" cellspacing="0" cellpadding="5" class="subpagetableborder">
                    <tr class="link_strip">
                      <td align="LEFT">
                        <div id="div" style="position:relative; display:inline;z-index:10000; float:left;"><strong>Country :</strong>
                          <%= text_field_with_auto_complete :analytics_country_list, :country_name, {:value=>@country_name,:maxlength => 20 }, {:method => :get,:skip_style => true} %> &nbsp;&nbsp;&nbsp;&nbsp;</div>
						  <div id="divAuto" style="position:relative; display:inline;z-index:10000; float:left;"><strong>Publisher:</strong>
						 <%= text_field_with_auto_complete :publisher, :publisher_name, {:value=>@pub_name,:maxlength => 20 }, {:method => :get,:skip_style => true} %>						 &nbsp;&nbsp;&nbsp;&nbsp;</div>						
						<div id="div2" style="position:relative; display:inline; float:left;">
						  <strong>Date:</strong>
                          <input readonly="true" type="text" id="search_date" name="search_date" size = '15' value="<%=@delivery_time%>" onClick="displayDatePicker('search_date', this, 'ymd', '-');"/>
                          <img src="/images/analytic/calander.gif" width="22" height="22" align="top" class="cal_height" onClick="displayDatePicker('search_date', document.getElementById('search_date'), 'ymd', '-');" border="0">  </div>&nbsp;&nbsp;&nbsp;&nbsp;<input type="submit" name="Submit" id="go" value="GO" />
